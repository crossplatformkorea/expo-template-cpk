    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.7",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.4.2",
    "react-test-renderer": "19.0.0",
