    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.4.2",
    "react-test-renderer": "^18.0.0",
    "ts-jest": "^29.2.5",
