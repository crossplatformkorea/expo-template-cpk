    "eslint": "^8.57.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.16",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.8.0",
    "react-test-renderer": "19.2.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
